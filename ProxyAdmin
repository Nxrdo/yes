		if g:IsA("Tool") then
			D:FireEvent("PompousTheCloud", game.Players.LocalPlayer.Character[g.Name].Handle, "Material", Enum.Material.ForceField)
			D:FireEvent("PompousTheCloud", game.Players.LocalPlayer.Character.PompousTheCloud.Handle, "Material", Enum.Material.Plastic)
		end
	end
end;

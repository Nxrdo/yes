local function TEJZ_fake_script() -- TL1.Script 
	local script = Instance.new('Script', TL1)

	function zigzag(X) return math.acos(math.cos(X*math.pi))/math.pi end
	counter = 0
	
	while wait(0.1) do
		script.Parent.TextColor3 = Color3.fromHSV(zigzag(counter),1,1)
		counter = counter + 0.005
	end
end
coroutine.wrap(TEJZ_fake_script)()
local function TPZZMG_fake_script() -- W1.Script 
	local script = Instance.new('Script', W1)

	script.Parent:TweenPosition(UDim2.new(0, 0, 0, 0), "Out", "Bounce", 2)
	
end
coroutine.wrap(TPZZMG_fake_script)()
